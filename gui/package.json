{
  "name": "DeZensur-gui",
  "version": "1.0.0",
  "description": "Electron/React Frontend",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm:electron-dev\"",
    "electron-dev": "electron .",
    "build": "vite build && electron-builder",
    "start": "electron ."
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "^24.13.1",
    "vite": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "tailwindcss": "^3.0.0",
    "postcss": "^8.0.0",
    "autoprefixer": "^10.0.0",
    "zustand": "^4.0.0",
    "react-dropzone": "^14.0.0",
    "react-router-dom": "^6.0.0",
    "concurrently": "^8.0.0",
    "electron-reload": "^2.0.0"
  },
  "build": {
    "appId": "com.dezensur.app",
    "productName": "DeZensur",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
